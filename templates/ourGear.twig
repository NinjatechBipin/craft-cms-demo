{% extends "index" %}
{% set body_classess = "inner-page" %}
{% block content %}


<article class="section">
      <div class="content-column content-column--narrow">

        <h1 class="centered">{{ entry.title }}</h1>
        {% set contentBlock = entry.contentBlock ? entry.contentBlock.all() %}

        {% include '_partials/_content_blocks' with {
          blocks: contentBlock
        } only %}
      </div>
    </article>
{% endblock %}